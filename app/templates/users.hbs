<h1>Our Authors</h1>
<div class="p-q">
  <div class="inner">
    {{#list-filter filter=(action 'filterByName') as |filteredResults|}}
      <div class="results">
        {{#each filteredResults as |user|}}
          <UsersList @user={{user}} />
        {{/each}}
      </div>
    {{/list-filter}}
    {{outlet}}
  </div>
</div>

